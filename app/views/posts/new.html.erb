<%= form_tag('/posts', method: :post) do %>
      <h3>
        投稿する
      </h3>
      <ul>
      <li><input placeholder="曲名" type="string" name="song_title"></li>
      <li><input placeholder="歌手" type="string" name="singer"></li>
      <li><input placeholder="Youtube Url" type="text" name="youtube_url"></li>
      <li><textarea cols="30" name="text" placeholder="text" rows="10"></textarea></li>
      <li><input type="submit" value="SENT"></li>
    </ul>
<% end %>
